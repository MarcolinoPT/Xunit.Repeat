<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <TargetFramework>netstandard1.1</TargetFramework>
        <Company></Company>
        <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
        <Description>Repeat attribute to use with Xunit</Description>
        <RepositoryUrl>https://github.com/MarcolinoPT/xUnit.Repeat</RepositoryUrl>
        <PackageProjectUrl>https://www.nuget.org/packages/Xunit.Repeat/</PackageProjectUrl>
        <PackageLicenseExpression>MIT</PackageLicenseExpression>
        <Authors>Marco de Freitas</Authors>
        <Version>1.1.0</Version>
        <AssemblyVersion>1.1.0.0</AssemblyVersion>
        <IsPackable>true</IsPackable>
    </PropertyGroup>
    <ItemGroup>
        <PackageReference Include="xunit" Version="2.4.1" />
    </ItemGroup>
</Project>
